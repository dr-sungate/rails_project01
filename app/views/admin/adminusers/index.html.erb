   <div class="pull-right">
    <%= link_to '新しい管理者を追加', new_admin_adminuser_path(), method: :get, class: "btn btn-default", style: "position:right; top:0; margin-bottom:10px;" %>
    </div>

 <table class="table table-striped table-vertical-middle">
    <thead>
      <tr>
        <th>メール</th>
       <th>ユーザー名</th>
         <th></th>
      </tr>
    </thead>
    <tbody>
    	<% if !@adminusers.nil? %>
      <% @adminusers.each do |adminuser| %>
          <tr>
          <td><%= adminuser.email %></td>
          <td><%= adminuser.name %></td>
           <td><%= link_to '編集', edit_admin_adminuser_path(adminuser), class: 'btn btn-info' %></td>
          <td><%= link_to '削除', admin_adminuser_path(adminuser.id), method: :delete,
                    data: { confirm: "「#{adminuser.name}」を解除します。よろしいですか？" },
                    class: 'btn btn-default' %></td>
           </tr>
       <% end %>
       <% end %>
    </tbody>
  </table>


